package com.view.verify;

public class ViewTest {
	
	public static void main(String[] args) {
		int test=16;
		switch (test%7) {
		case 2:
			System.out.println("<<<<2222222<<");
			break;
		case 0:
			System.out.println("<<<<2222222<<");
			break;
		case 1:
			System.out.println("<<<<2222222<<");
			break;
		case 3:
			System.out.println("<<<<2222222<<");
			break;
		case 4:
			System.out.println("<<<<2222222<<");
			break;
		case 5:
			System.out.println("<<<<2222222<<");
			break;
		case 6:
			System.out.println("<<<<2222222<<");
			break;
		default:
			System.out.println("<<<<default<<");
			break;
		}
		
	}

}
